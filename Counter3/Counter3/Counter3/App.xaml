<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Counter3.App">
    <Application.Resources>
        <Color x:Key="Primary">#F5DEB3</Color>
        <Color x:Key="Secondary">#E97451</Color>
        <Color x:Key="Tertiary">#B87333</Color>
        <Color x:Key="Dark">#6F4E37</Color>

        <Style TargetType="NavigationPage">
            <Setter Property="BarBackgroundColor" Value="{StaticResource Tertiary}" />

        </Style>
    </Application.Resources>
</Application>
